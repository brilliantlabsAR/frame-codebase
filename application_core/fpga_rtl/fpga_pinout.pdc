ldc_set_location -site {D3} [get_ports cipo]
ldc_set_location -site {C3} [get_ports copi]
ldc_set_port -iobuf {IO_TYPE=LVCMOS18} [get_ports sck]
ldc_set_port -iobuf {IO_TYPE=LVCMOS18} [get_ports cs]
ldc_set_port -iobuf {IO_TYPE=LVCMOS18} [get_ports copi]
ldc_set_port -iobuf {IO_TYPE=LVCMOS18} [get_ports cipo]

